{
  "name": "unpdfer",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "start1": "node ./scripts/start.js",
    "start": "node --inspect=0.0.0.0:9229 -r ts-node/register ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --config moleculer.config.ts services/**/*.service.ts",
    "start:es": "node --inspect=0.0.0.0:9229 -r ts-node/register ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --config ./out/moleculer.config.js ./out/services/**/*.service.js",
    "watch:es": "esbuild ./moleculer.config.js ./services/*.ts ./src/*.ts ./src/**/*.ts  --format=cjs --platform=node --outdir=out --legal-comments=none --watch",
    "build:es": "esbuild ./moleculer.config.js ./services/*.ts ./src/*.ts ./src/**/*.ts  --format=cjs --platform=node --outdir=out --legal-comments=none",
    "test": "ts-node ./benchmark.ts"
  },
  "dependencies": {
    "@d0whc3r/moleculer-telegram": "^1.2.0",
    "@moleculer/lab": "^0.6.2",
    "@types/archiver": "^5.1.1",
    "@types/ftp": "^0.3.32",
    "@types/mkdirp": "^1.0.0",
    "@types/unzip-stream": "^0.3.1",
    "@types/url-join": "^4.0.1",
    "@types/uuid": "^8.3.1",
    "archiver": "^5.3.0",
    "axios": "^0.21.4",
    "benchmarkify": "^3.0.0",
    "cbor-x": "^0.8.3",
    "csv-parse": "^4.16.3",
    "duplexify": "^4.1.2",
    "esbuild": "^0.13.4",
    "fast-xml-parser": "^3.20.3",
    "ftp": "^0.3.10",
    "ghostscript4js": "^3.2.1",
    "iconv-lite": "^0.6.3",
    "libxmljs2": "^0.28.0",
    "moleculer": "^0.14.17",
    "moleculer-decorators": "^1.3.0",
    "moleculer-minio": "^1.0.0",
    "moleculer-web": "^0.10.2",
    "nats": "^1.3.2",
    "ts-node": "^8.8.1",
    "ts-transformer-json-schema": "^0.6.12",
    "typescript": "^4.4.3",
    "unzip-stream": "^0.3.1",
    "url-join": "^4.0.1",
    "uuid": "^8.3.2",
    "xml-reader": "^2.4.3"
  },
